{
  "name": "consolejs",
  "version": "1.2.13",
  "description": "JavaScript library that overrides the console object bringing its functionality in a DOM element.",
  "main": "lib/console.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IonicaBizau/console.js.git"
  },
  "keywords": [
    "console",
    "html",
    "library"
  ],
  "author": "Ionică Bizău <bizauionica@gmail.com> (https://ionicabizau.net)",
